{
    "AccessKey": {
        "description": "Access密钥",
        "type": "string",
        "hint": "请前往  https://www.liblib.art/apis 注册账号，并获取 AccessKey",
        "default": "AccessKey",
        "obvious_hint": true
    },
    "SecretKey": {
        "description": "Secret密钥",
        "type": "string",
        "default": "SecretKey",
        "hint": "请前往  https://www.liblib.art/apis 注册账号，并获取 AccessKey"
    },
    "modelId": {
        "description": "模型ID",
        "type": "string",
        "default": "0ea388c7eb854be3ba3c6f65aac6bfd3",   
        "hint": "https://www.liblib.art点击要使用的模型,在浏览器url处获取模型链接结尾的version_uuid"
    },
    "height": {
        "description": "生成图片的高度",
        "type": "int",
        "default": "1024",
        "hint": "图片分辨率的高度"
    },
    "width": {
        "description": "生成图片的宽度",
        "type": "int",
        "default": "1024",
        "hint": "图片分辨率的宽度"
    },
    "num_inference_steps": {
        "description": "推理步数",
        "type": "int",
        "default": 20,
        "hint": "生成图片时的推理步数，数值越大生成质量越高但速度越慢"
    },
    "seed": {
        "description": "种子ID",
        "type": "string",
        "default": "随机",
        "hint": "用于控制生成结果的随机性，输入'随机'则自动生成随机种子"
    },
    "imgType": {
        "description": "生成图片的模式，",
        "type": "int",
        "default": 0,
        "hint": "0=1.5/XL模式(可自定义模型)，1=flux模式，2=confyui模式"
    }
}
