{
    "AccessKey": {
        "description": "Access密钥",
        "type": "string",
        "hint": "请前往  https://www.liblib.art/apis 注册账号，并获取 AccessKey",
        "default": "AccessKey",
        "obvious_hint": true
    },
    "SecretKey": {
        "description": "Secret密钥",
        "type": "string",
        "default": "SecretKey",
        "hint": "请前往  https://www.liblib.art/apis 注册账号，并获取 AccessKey"
    },
    "modelId": {
        "description": "模型ID",
        "type": "string",
