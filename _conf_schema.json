{
    "AccessKey": {
        "description": "Access密钥",
        "type": "string",
        "hint": "请前往  https://www.liblib.art/apis 注册账号，并获取 AccessKey",
        "default": "AccessKey",
        "obvious_hint": true
    },
    "SecretKey": {
        "description": "Secret密钥",
        "type": "string",
        "default": "black-forest-labs/FLUX.1-dev",
        "hint": "AI模型的名称"
    },
    "height": {
        "description": "生成图片的高度",
        "type": "string",
        "default": "1024",
        "hint": "图片分辨率的高度"
    },
    "width": {
        "description": "生成图片的宽度",
        "type": "int",
        "default": "1024",
        "hint": "图片分辨率的宽度"
    },
    "num_inference_steps": {
        "description": "推理步数",
        "type": "int",
        "default": 20,
        "hint": "生成图片时的推理步数，数值越大生成质量越高但速度越慢"
    },
    "seed": {
        "description": "种子ID",
        "type": "string",
        "default": "随机",
        "hint": "用于控制生成结果的随机性，输入'随机'则自动生成随机种子"
    },
    "enable_translation": {
        "description": "启用中文翻译",
        "type": "bool",
        "default": true,
        "hint": "是否自动将中文提示词翻译成英文提示词"
    }
}
